{
  "author": "gucong3000",
  "bugs": {
    "url": "https://github.com/gucong3000/git-win/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "fs-extra": "^5.0.0",
    "got": "^8.0.3",
    "nugget": "^2.0.1"
  },
  "description": "Install Git for Windows by npm.",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "proxyquire": "^1.8.0"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/gucong3000/git-win#readme",
  "keywords": [
    "git",
    "window",
    "path"
  ],
  "preferGlobal": true,
  "license": "MIT",
  "main": "./lib/index.js",
  "name": "git-win",
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true
  },
  "os": [
    "win32"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gucong3000/git-win.git"
  },
  "scripts": {
    "preinstall": "node -e \"require('util').promisify.call\" || npm i util.promisify",
    "install": "node lib/install.js",
    "build": "rm -rf lib *.tgz && babel src --out-dir lib && cp src/*.json lib",
    "prepare": "npm run build",
    "pretest": "eslint src test",
    "report-coverage": "codecov",
    "test": "nyc mocha --no-timeouts test/index.js"
  },
  "version": "1.5.1",
  "optionalDependencies": {
    "util.promisify": "^1.0.0"
  }
}
