{
  "author": "gucong3000",
  "bugs": {
    "url": "https://github.com/gucong3000/git-win/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "fs-extra": "^7.0.1",
    "got": "^9.5.0",
    "in-gfw": "^1.2.0",
    "nugget": "^2.0.1"
  },
  "description": "Install Git for Windows by npm.",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.1.0",
    "eclint": "^2.8.1",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.0"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/gucong3000/git-win#readme",
  "keywords": [
    "git",
    "window",
    "resolve",
    "path",
    "posix"
  ],
  "preferGlobal": true,
  "license": "MIT",
  "main": "./lib/index.js",
  "name": "git-win",
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true
  },
  "os": [
    "win32",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gucong3000/git-win.git"
  },
  "scripts": {
    "install": "node lib/install.js",
    "build": "rm -rf lib *.tgz && babel src --out-dir lib && cp src/*.json lib",
    "prepare": "npm run build",
    "lint:eclint": "eclint check $(git ls-files | tee /tmp/git-files)",
    "lint:eslint": "eslint $(grep \"\\.js$\" /tmp/git-files)",
    "lint": "npm run lint:eclint && npm run lint:eslint",
    "pretest": "env npm run lint --script-shell=/bin/sh",
    "report-coverage": "codecov",
    "test": "npm run unit",
    "unit": "nyc mocha --no-timeouts test/index.js"
  },
  "version": "2.3.0"
}
